{
  "name": "developer-preview",
  "version": "1.1.0",
  "description": "Simple URL Preview for Developers.",
  "main": "electron/main.js",
  "scripts": {
    "elec": "electron .",
    "nodemon": "nodemon --exec electron .",
    "build": "electron-builder -p=never",
    "publish": "electron-builder -p=always"
  },
  "build": {
    "productName": "Developer Preview",
    "appId": "com.alaskcode.developerpreview",
    "nsis": {
      "publish": {
        "provider": "github",
        "owner": "Alask-code",
        "repo": "Developer-Preview"
      },
      "oneClick": false
    }
  },
  "author": "Alask-code",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@electron-internal/eslint-config": "^1.0.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^3.0.3"
  }
}
